{% extends 'AceEditorBundle:Default:container.html.twig' %}
{% block title "The Team" %}
{% block mainspan %}
<div class="page-header">
  <h1>Meet The Team!<small> The guys and gals behind the stuff</small></h1>
</div>
{% for i in developers %}
<div class="row-fluid">
	<div class="span4">
		<ul class="thumbnails">
			<li class="span12">
			<div class="thumbnail">
				<img src="{{ i.image }}" alt="">
			</div>
			</li>
		</ul>
	</div>
	<div class="span8">
		<h2>{{ i.name }}<small> {{i.subtext}}</small></h2>
		<p>{{ i.description }}</p>
	</div>
</div>
{% endfor %}

{% endblock %}